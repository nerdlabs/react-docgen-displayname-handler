{
  "name": "react-docgen-displayname-handler",
  "version": "0.0.0-development",
  "description": "A handler for react-docgen that tries to infer the displayName of a component",
  "license": "MIT",
  "author": "nerdlabs <nerds@nerdlabs.it>",
  "keywords": [
    "react",
    "documentation-generation",
    "react-docgen"
  ],
  "homepage": "https://github.com/nerdlabs/react-docgen-displayname-handler#readme",
  "bugs": {
    "url": "https://github.com/nerdlabs/react-docgen-displayname-handler/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nerdlabs/react-docgen-displayname-handler.git"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "source"
  ],
  "main": "source/index.js",
  "scripts": {
    "lint": "eslint source/*",
    "fmt": "prettier --write source/*.js",
    "test:unit": "node source/index.spec.js",
    "test": "npm run test:unit && tav"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:node/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  },
  "renovate": {
    "extends": [
      "config:js-lib",
      ":prNotPending",
      ":maintainLockFilesWeekly",
      ":automergeLinters",
      ":automergeTesters",
      ":automergeTypes"
    ],
    "packageRules": [
      {
        "updateTypes": [
          "lockFileMaintenance"
        ],
        "automerge": true
      }
    ]
  },
  "peerDependencies": {
    "react-docgen": "^3.0.0 || ^4.0.0 || ^5.0.0-beta || ^5.0.0"
  },
  "devDependencies": {
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "prettier": "1.19.1",
    "react-docgen": "5.0.0",
    "test-all-versions": "4.1.1"
  },
  "dependencies": {
    "ast-types": "0.13.2"
  }
}
